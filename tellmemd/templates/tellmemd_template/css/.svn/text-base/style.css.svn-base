/*Main Menu*/

.main_menu ul li {
	display:inline;
	padding-right:5px;
}
.main_menu ul li a {
	color: #FFFFFF;
	font-size: 18px;
	line-height: 37px;
	padding:12px 20px;
	text-decoration: none;
	background-image:url(../images/menu_bg.jpg);
	background-repeat:repeat-x;
	border-radius: 10px 10px 0px 0px;
}
.main_menu ul li a:hover, .main_menu ul li.current a{
	background-image:url(../images/menu_bg.jpg);
	background-repeat:repeat-x;
	opacity: 0.7;
	}
/*Loged in Panel*/
.moduletable_logedin_panel {
	text-align:right;
}
.moduletable_logedin_panel ul li {
	display:inline;
}
.moduletable_logedin_panel ul li a {
	text-decoration:none;
	padding:10px 15px 10px 15px;
	line-height:25px;
	background-color:#212121;
	font-size:14px;
	color:#FFF;
	background-image:url(../images/logedin_bg.jpg);
	background-position:center right;
	background-repeat:no-repeat;
}
.moduletable_logedin_panel ul li a:hover, .moduletable_logedin_panel ul li.active a{
	color:#FC3;
	}
.moduletable_logedin_panel ul li.item-105 a {
	border-radius:0px 0px 0px 10px;
}
.moduletable_logedin_panel ul li.item-107 a {
	border-radius:0px 0px 10px 0px;
	background-image:none;
}
/*Header Verticle Menu*/
.moduletable_header_verticle {
	padding:0px 10px;
	background-image:url(../images/verticle_menu_bg.jpg);
	background-repeat:repeat-x;
	background-position:bottom;
	border-radius:10px;
	margin-top:10px;
	border:2px solid #d4efff;
}
.moduletable_header_verticle ul li {
	background-image:url(../images/verticle_menu_li_bg.png);
	background-position:center right;
	background-repeat:no-repeat;
	list-style:none;
	padding-right:20px;
}
.moduletable_header_verticle ul li a {
	text-decoration:none;
	font-weight:600;
	color:#2b2b2b;
}
.moduletable_header_verticle ul li span.image-title {
	font-size: 16px;
	text-decoration: none;
	vertical-align: top;
	margin-left:10px;
	vertical-align:top;
	line-height:40px;
}

/*Footer Menu*/
.moduletable_footer_menu ul li{
	display:inline;
	padding-left:10px;
	padding-right:10px;
	border-right:1px solid #FFF;
	}
.moduletable_footer_menu ul li.item-116{
	border:none;
	}
.moduletable_footer_menu ul li a{
	text-decoration:none;
	color:#FFF;

	}
.moduletable_footer_menu{
	padding-top:10px;
	}
.footer_menu a{
	text-decoration:none;
	color:#fff;
	}
.footer_menu a:hover{
	text-decoration:underline;
	}
