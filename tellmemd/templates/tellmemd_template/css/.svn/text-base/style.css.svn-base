@charset "utf-8";
/* CSS Document */
* {
	margin:0px;
	padding:0px;
	font-family:"Myriad Pro";
	font-size:12px;
	color:#4b4b4b;
}
/*Basic Layout*/
body {
	border-top:5px solid #212121;
}
.main_wrapper {
	width:985px;
	margin-left:auto;
	margin-right:auto;
}
.header_wrapper {
	height:auto;
}
.login_panel {
	height:35px;
}
.main_menu {
	height:auto;
	/*border-bottom:5px solid #42baff;*/
	margin-bottom:10px;
/*	margin-top:-10px;*/     /*Dev Rafi - removed to align the menu correctly*/
}
