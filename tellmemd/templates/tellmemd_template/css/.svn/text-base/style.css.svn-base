/*Question Pane - Two Tabs*/
.tabs {
	float:left;	
}
ul.tabs li#first, ul.tabs li#first a:hover, ul.tabs li#first.active a{
	border-radius: 10px 0px 0px 0px;
	}
ul.tabs li#second , ul.tabs li#second a:hover, ul.tabs li#second.active a{
	border-radius: 0px 10px 0px 0px;
	}
.tabs li {
	float:left;
	position:relative;
	overflow:hidden;
	background:#990000;
}
.tabs li a {
	display:block;
	width:200px;
	text-align:center;
	padding:13px 0 12px;
	font-size:20px;
	line-height:20px;
	text-decoration:none;
	color:#fff;
}
.tabs li:hover {
	background:#000000;
}
.tabs li.active a, .tabs li.active a:hover {
	color:#fff;
	background:#000;
}
.tab_container {
	width:100%;
	color:#fff;
	overflow:hidden;
	background:#2d2d2d;
	border-radius: 0px 10px 10px 10px;
	height:330px;
}
.padding_container {
	padding:0px 0px 10px 0px;
	background-color:#000;

}
.wrapper {
	width:550px;
	overflow:hidden;
}
.tab_content{
	width:200px;
	float:left;
	}
.padding_container ul.submenu li{
	list-style:none;
	padding:10px 5px 10px 10px;
	background-image:url(../images/bullet_question_pane.png);
	background-position:center right;
	background-repeat:no-repeat;
	border-bottom:1px solid #2b2b2b;
	cursor:pointer;
	}
.padding_container ul.submenu li.active{
	background-color:#2b2b2b;
	}
.padding_container ul.submenu li#last{
	border:none;
	}
.padding_container ul.submenu li a{
	text-decoration:none;
	font-size:14px;
	color:#fff;
	}
.padding_container ul.submenu li#last a{
	font-size:30px;
	}
